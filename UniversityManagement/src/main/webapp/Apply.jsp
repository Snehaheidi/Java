<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>collegelist</title>
<link rel="stylesheet" type="text/css" href="style/style.css">
</head>
<body>
	<div class="container">
		<h1>CollegeList</h1>
		<form>
			<div class="innercontainer">
				<label>Enter your Name : </label> <input type="text" id="name"
					name="name"><br> <label>Enter Date of Birth :
				</label> <input type="text" id="dob" name="dob"><br> <label>Enter
					the Mail id : </label> <input type="text" id="mailid" name="mailid"><br>
				<label>Enter your Phone Number : </label> <input type="text"
					id="phNumber" name="phNumber"><br> <label>Enter
					your District : </label> <input type="text" id="dist" name="dist"><br>
				<label>Enter School Name : </label> <input type="text" id="schlname"
					name="schlname"><br> <label>Enter the 10th
					Percentage : </label><input type="text" id="tenper" name="tenper"><br>
				<label>Enter the 12th Percentage : </label><input type="text"
					id="twlper" name="twlper"><br> <label>Enter
					your city : </label> <input type="text" id="city" name="city"><br>
				<label>Select College : </label> <input type="text" id="clg" name="clg"><br> 
				<button id="submit" name="submit">Submit</button>
			    <br>
			</div>
		</form>
	</div>
</body>
</html>